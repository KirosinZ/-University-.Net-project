@model int

@{ 
    ViewBag.Title = "Удаление пользователя";
}

<h3>Вы уверены, что хотите удалить пользователя?</h3>
<table style="width: 20%">
    <tr>
        <td>
            <a asp-action="DeleteConfirm" asp-route-id="@Model" class="btn btn-outline-dark">Да</a>
        </td>
        <td>
            <a asp-action="Index"  asp-route-id="@Model" class="btn btn-outline-dark">Отмена</a>
        </td>
    </tr>
</table>
